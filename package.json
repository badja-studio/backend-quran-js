{
  "name": "backend-quran-js",
  "version": "1.0.0",
  "description": "Backend Quran API with Express, PostgreSQL, and Sequelize",
  "main": "cmd/web/main.js",
  "scripts": {
    "start": "node cmd/web/main.js",
    "dev": "nodemon cmd/web/main.js",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "db:seed:undo": "sequelize-cli db:seed:undo",
    "db:seed:undo:all": "sequelize-cli db:seed:undo:all",
    "db:seed:new": "node seeders/seed-new-data.js"
  },
  "keywords": [
    "quran",
    "api",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.1.0",
    "html-pdf": "^3.0.1",
    "html2canvas": "^1.4.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.4",
    "morgan": "^1.10.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "phantomjs-prebuilt": "^2.1.16",
    "puppeteer": "^24.31.0",
    "sequelize": "^6.35.2",
    "sequelize-cli": "^6.6.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "xlsx2json": "^1.0.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.0.2"
  }
}
